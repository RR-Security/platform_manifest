<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com"
           review="android-review.googlesource.com"
           revision="refs/tags/android-6.0.1_r81" />

  <remote  name="github"
           fetch="https://github.com" />
 
  <remote  name="cm"
           fetch="git://github.com/LineageOS"
           revision="cm-13.0"
           review="review.lineageos.org" />

  <remote  name="android-security"
           fetch="https://github.com/android-security"
           revision="aosp-6.0" />

  <remote  name="rr"
	       fetch="https://github.com/RR-Security"
	       revision="marshmallow" />

  <default revision="refs/heads/cm-13.0"
           remote="github"
           sync-c="true"
           sync-j="4"
	       sync-f="true" />

<!-- Include RR-Security manifest -->
  <include name="rr-security.xml" />
  
<!-- Include LineageOS manifest -->
  <include name="lineage.xml" />

<!-- Include AOSP manifest -->
  <include name="aosp.xml" />
</manifest>
